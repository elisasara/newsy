<div class="container">
    {{#each articleObj}}
    <div class="row">
        <div class="col-md-8">
            <h2>{{this.title}}</h2>
            <br />
            <a href={{this.link}}>{{this.link}}</a>
        </div>
        <div class="col-md-4">
            <button class="btn">Add Note</button>
            <br />
            <button class="btn">Save Article</button>
        </div>
    </div>
    {{/each}}
</div>