<div class="container">
    {{#each articleObj}}
    <div class="row">
        <div class="col-md-8">
            <h2>{{this.title}}</h2>
            <br />
            <a href={{this.link}}>{{this.link}}</a>
        </div>
        <div class="col-md-4">
            <a href="/notes"><div class="btn btn-primary addNote" data-id="{{this._id}}">Add Note</div></a>
            <br />
            <div class="btn btn-success save" data-id="{{this._id}}">Save Article</div>
        </div>
    </div>
    {{/each}}
</div>