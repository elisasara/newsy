<div class="container">
    {{#each articleObj}}
    <div class="row">
        <div class="col-md-8">
            <h2>{{this.title}}</h2>
            <br />
            <a href={{this.link}}>{{this.link}}</a>
        </div>
        <div class="col-md-4">
            <div class="btn btn-primary addNote" id="{{this._id}}">Add Note</div>
            <br />
            <div class="btn btn-success" id="{{this._id}}">Save Article</div>
        </div>
    </div>
    {{/each}}
</div>